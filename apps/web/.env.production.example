# Production Environment Variables for Vercel Deployment
# Copy this file and configure with your actual production values

# Core Configuration
NODE_ENV=production
NEXT_TELEMETRY_DISABLED=1

# API Configuration
NEXT_PUBLIC_API_URL=https://your-api-domain.up.railway.app
NEXT_PUBLIC_APP_URL=https://your-app.vercel.app

# Optional Features
NEXT_PUBLIC_ENABLE_ANALYTICS=true
NEXT_PUBLIC_ENABLE_ERROR_REPORTING=true
NEXT_PUBLIC_ENABLE_SW=true

# Build Configuration
NEXT_PUBLIC_BUILD_TIME=2025-01-08T00:00:00Z
NEXT_PUBLIC_VERSION=1.0.0